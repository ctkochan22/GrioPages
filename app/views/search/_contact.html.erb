<div id="contact_container">
  <h3>Contact:</h3>
  <h4>Name:</h4>
  <p><%= @contact.name %></p>

  <h4>Address:</h4>
  <p><%= @contact.line1 %></p>
  <% if @contact.line2.length > 0 %>
    <p><%= @contact.line2 %></p>
  <% end %>
  <p><%= @contact.city %>, <%= @contact.state %> <%= @contact.zip %></p>

  <h4>Phone Number:</h4>
  <p><%= @contact.phone %></p>
</div>